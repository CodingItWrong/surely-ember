<PaperSidenavContainer @class="full-height">
  {{yield (hash
    sidenav=(component 'layout-sidenav' open=this.leftSideBarOpen onToggle=(action (mut this.leftSideBarOpen)))
    body=(component 'layout-body')
    toolbar=(component 'layout-toolbar' onToggleSidebar=(action (mut this.leftSideBarOpen)))
    content=(component 'layout-content')
  )}}
</PaperSidenavContainer>
