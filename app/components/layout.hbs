<PaperSidenavContainer @class="full-height">
  {{yield (hash
    sidenav=(component 'layout-sidenav'
      title=@title
      open=this.sideNavOpen
      onToggle=(action (mut this.sideNavOpen))
    )
    body=(component 'layout-body'
      title=@title
      onToggleSideNav=(action (mut this.sideNavOpen))
    )
  )}}
</PaperSidenavContainer>
