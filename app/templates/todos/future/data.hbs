<TodoList
  @title="Future"
  @icon="calendar_today"
  @groups={{this.todoGroups}}
  @searchAvailable={{true}}
  @onSearch={{this.handleSearch}}
  @noTodosMessage="You have no future todos. Nice work!"
  @onClickHeaderIcon={{this.goToList}}
  @onChooseTodo={{this.handleChooseTodo}}
  @onRefresh={{this.handleRefresh}}
/>
{{outlet}}
