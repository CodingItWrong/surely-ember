<div class="list-detail">
  <TodoList
    @title="Completed"
    @icon="check_circle"
    @todos={{this.sortedTodos}}
    @onClickHeaderIcon={{this.goToList}}
    @onChooseTodo={{this.handleChooseTodo}}
    @onRefresh={{action send 'refresh'}}
    as |todo|
  >
    Completed {{time-ago todo.completedAt}}
  </TodoList>
  {{outlet}}
</div>
