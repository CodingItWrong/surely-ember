<TodoList
  @title="Completed"
  @icon="check_circle"
  @todos={{this.sortedTodos}}
  @noTodosMessage="You have no completed todos. You'll get there!"
  @onClickHeaderIcon={{this.goToList}}
  @onChooseTodo={{this.handleChooseTodo}}
  @onRefresh={{this.handleRefresh}}
  as |todo|
>
  Completed {{time-ago todo.completedAt}}
</TodoList>
{{outlet}}
